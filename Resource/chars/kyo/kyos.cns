hitsound = s0,21                  ;ヒットした時の音（ｓ*,*にすると自分の音を使う）
guardsound = s0, 5                ;ガードした時の音（同上）
ground.type = High               ;相手のくらった場所（High：頭、体, Low：足, Trip：足払い）
